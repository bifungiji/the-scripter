-- Gui to Lua
-- Version: 3.2

-- Instances:


print("JUMPS STARTED")

local Screeny = Instance.new("ScreenGui")
local Jumpscare2 = Instance.new("ImageLabel")
local Jumpscare1 = Instance.new("ImageLabel")
local Jumpscare3 = Instance.new("ImageLabel")









Screeny.Name = "Screeny"
Screeny.Parent = game.CoreGui
Screeny.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Jumpscare2.Name = "Jumpscare2"
Jumpscare2.Parent = Screeny
Jumpscare2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Jumpscare2.Size = UDim2.new(1, 0, 1, 0)
Jumpscare2.Visible = false
Jumpscare2.Image = "rbxassetid://10111106414"

Jumpscare1.Name = "Jumpscare1"
Jumpscare1.Parent = Screeny
Jumpscare1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Jumpscare1.Size = UDim2.new(1, 0, 1, 0)
Jumpscare1.Visible = false
Jumpscare1.Image = "rbxassetid://10111124457"

Jumpscare3.Name = "Jumpscare3"
Jumpscare3.Parent = Screeny
Jumpscare3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Jumpscare3.Size = UDim2.new(1, 0, 1, 0)
Jumpscare3.Visible = false
Jumpscare3.Image = "rbxassetid://10111159872"

while wait(1) do
if _G.Dark == false then
print("KILLED GUI")
Screeny:Destroy()
end
end

while wait(1) do
if _G.Dark == true then


SAM = math.random(1,50) 
pic = math.random(1,3) 
print(SAM)
wait(SAM)

if pic == 1 then
Jumpscare1.Visible = true

wait(5)
Jumpscare1.Visible = false
else
if pic == 2 then
Jumpscare2.Visible = true
wait(5)   
Jumpscare2.Visible = false
else
Jumpscare3.Visible = true
wait(5)
Jumpscare3.Visible = false



end
end 
end
end
