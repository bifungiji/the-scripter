

 TheUser = game:GetService("Players").LocalPlayer.Name
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(mes)
if Player.Name ==  "tipthedripper" or "FIRESTORMGAMERYT5" or "ftk_choppa4x" or "XxOmniversalxX"  or "whyjustwhy2112"  then
mes = string.lower(mes)
local split = mes:split(" ")
local cmd = split[1]
if cmd == "]usingc"
then
target = split[2]
if target == "all" then 
 local ohString1 =    "/w " ..Player.Name.. " Im Using Cylindrical"
local ohString2 = "All"
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ohString1, ohString2)
else   
  for i,v in pairs(game.Players:GetPlayers()) do 
                 if v.Name:lower():sub(1,#target) == target:lower() then
                   target = v.Name
if v.Name == TheUser then
 local ohString1 =    "/w " ..Player.Name.. " Im Using Cylindrical"
local ohString2 = "All"
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ohString1, ohString2)
end
end
end
end
end
end
end)
end)










  TheUser = game:GetService("Players").LocalPlayer.Name
game.Players.PlayerAdded:connect(function(Player)
    Player.Chatted:connect(function(mes)
if Player.Name ==  "tipthedripper" or "FIRESTORMGAMERYT5" or "ftk_choppa4x" or "XxOmniversalxX" or "whyjustwhy2112"    then
mes = string.lower(mes)
local split = mes:split(" ")
local cmd = split[1]
if cmd == "]kill"
then
target = split[2]
if target == "all" then 
game:GetService("Workspace")[TheUser].Head:Destroy()
else
  for i,v in pairs(game.Players:GetPlayers()) do 
                 if v.Name:lower():sub(1,#target) == target:lower() then
                   target = v.Name
if v.Name == TheUser then
game:GetService("Workspace")[TheUser].Head:Destroy()
end
end
end
end
end
end
end)
end)





