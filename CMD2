loadstring(game:HttpGet('https://github.com/bifungiji/the-scripter/raw/main/CMD2'))()
]]

Place = game.placeId
job = game.JobId
game:GetService("TeleportService"):TeleportToPlaceInstance(Place, job, game.Players.LocalPlayer)



end
end
end
end
end
end
end)
